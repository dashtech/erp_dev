              inherit_id="sale.report_saleorder">
        <xpath expr="//t[@t-foreach='docs']" position="before">
            <t t-set="header_label" t-value="'ĐƠN HÀNG'"/>
        </xpath>
    </template>

    <template id="report_saleorder_quotation">
        <t t-call="report.html_container">
            <!--<t t-set="header_label" t-value="'BÁO GIÁ'"/>-->
            <t t-foreach="docs" t-as="doc">
                <t t-set="header_label" t-value="'BÁO GIÁ'"/>
                <t t-if="doc.create_form_fleet == True">
                    <t t-set="header_label" t-value="'BÁO GIÁ CHI PHÍ SỬA CHỮA BẢO DƯỠNG XE'"/>
                </t>
                <t t-call="sale.report_saleorder_document" t-lang="doc.partner_id.lang"/>
            </t>
        </t>
    </template>

